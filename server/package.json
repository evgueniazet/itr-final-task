{
    "name": "pictures-server",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "nodemon --exec \"npm run build && node ./build/index.js\" --watch src --ext ts",
        "build": "rm -rf build && tsc -p tsconfig.json",
        "db:migrate": "sequelize-cli db:migrate",
        "db:migration:create": "ts-node ./src/scripts/migrationCreate.ts",
        "lint:ts": "eslint . --ext .ts,.tsx",
        "lint:js": "eslint --max-warnings 0 -f table --ext .ts src/ && npm run lint:ts"
    },
    "author": "evgueniazet",
    "license": "ISC",
    "_moduleAliases": {
        "src": "build/src",
        "config": "build/config",
        "enums": "build/enums",
        "interfaces": "build/interfaces",
        "models": "build/models",
        "routes": "build/routes",
        "types": "build/types"
    },
    "dependencies": {
        "@types/node": "^20.11.22",
        "@types/sequelize": "^4.28.20",
        "@typescript-eslint/eslint-plugin": "^7.1.0",
        "@typescript-eslint/parser": "^7.1.0",
        "axios": "^1.6.7",
        "dotenv": "^16.4.5",
        "eslint": "^8.57.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^9.1.0",
        "eslint-formatter-table": "^7.32.1",
        "express": "^4.18.2",
        "module-alias": "^2.2.3",
        "mysql2": "^3.9.2",
        "nodemon": "^3.1.0",
        "prettier": "^3.2.5",
        "sequelize": "^6.37.1",
        "sequelize-cli": "^6.6.2",
        "typescript": "^5.3.3"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "tslib": "^2.6.2"
    }
}
